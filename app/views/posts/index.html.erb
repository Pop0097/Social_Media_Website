<!-- User will be able to access their profile and all here -->
<section id = "recents_title">
  <h1> Recents </h1>
</section>

<article>
  <table id = "recents_display">
    <tr>
    </tr>
    <% @posts.each do |post| %>
      <tr>
        <td>
          <%= post.user.username %>:
            <%= link_to post.title, post_path(post), id: "recents_display_title_links" %>
          <p id = "recents_display_body">
            <%= post.body %>
          </p>
        </td>
      </tr>
    <% end %>
  </table>
</article>
